// --------------------
//
//
// * CONFIG
//
//
// --------------------
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// --------------------
//
//
// * ENUMS
//
//
// --------------------
enum UserRole {
  USER
  MANAGER
  ADMIN
}

enum AccountType {
  FAN
  PERFORMER
  VENUE
  ORGANIZER
}

enum ProfileType {
  MUSICIAN
  BAND
  VENUE
  ORGANIZER
}
